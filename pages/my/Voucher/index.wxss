/* pages/my/Voucher/index.wxss */

/**index.wxss**/

.swiper-tab {
  width: 100%;
  text-align: center;
  line-height: 36px;

}

.swiper-tab-list {
  font-size: 30rpx;
  display: inline-block;
  width: 33%;
  color: #7a7a7a;
}

.on {
  color: #000004;
  border-bottom: 8rpx solid #fde800;
}
swiper-items{
  height: 100%;
  
}
.scroll-views{
  height: 100%;
  
}
.swiper-box {
  display: block;
  min-height: 100%;
  height: 2000rpx;
  width: 100%;
  background-color: #f1f2f5;
}

.table {
  width: 100%;
  padding: 3%;
  height: 100%;
}

.voucher {
  width: 28%;
  margin-bottom: 3%;
  float: left;
  height: 190rpx;
   background-image: url("data:image/png;base64,");
}

.voucher2 {
  width: 28%;
  margin-bottom: 3%;
  float: left;
  height: 190rpx;
   background-image: url("data:image/png;base64,");
}

.tex {
  position: relative;
  left: 20%;
  top: 21%;
  font-size: 30rpx;
  font-weight: 500;
}

.tex text {
  font-size: 85rpx;
  font-weight: 500;
}

.wen {
  height: 190rpx;
  width: 65%;
  margin-bottom: 3%;
  margin-right: 7%;
  float: right;
  background-color: white;
}

.wen image {
  width: 110rpx;
  height: 60rpx;
  position: relative;
  top: 28%;
  right: 0;
}

.wen1 {
  width: 100%;
  text-align: left;
  font-size: 38rpx;
  line-height: 40px;
  height: 100rpx;
  font-weight: 500;
  color: #000;
  padding: 10% 3% 3% 3%;
}

.wen2 {
  font-size: 24rpx;
  line-height: 24rpx;
  font-weight: 400;
  height: 90rpx;
  color: #868686;
  padding: 0% 3% 0% 3%;
}
/*隐藏滚动条*/
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}